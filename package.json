{
	"name": "car-garage-app",
	"version": "1.0.0",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "cross-env ENV=development expo start",
		"android": "cross-env ENV=development expo start --android",
		"ios": "cross-env ENV=development expo start --ios",
		"lint": "eslint . --fix",
		"test": "jest",
		"eject": "cross-env ENV=development expo eject",
		"build": "eas build",
		"build:development": "eas build --profile development --platform android",
		"build:preview": "eas build --profile preview",
		"build:apk": "expo build:android -t apk",
		"svgr": "yarn svgr:remove-icons && yarn svgr:generate-icons && yarn svgr:rename-index",
		"svgr:generate-icons": "svgr --config-file ./.svgr/svgr.config.js ./src/media/icons/",
		"svgr:remove-icons": "rimraf ./src/components/icons",
		"svgr:rename-index": "find ./src/components/icons | renamer --find index.icon.tsx --replace index.tsx",
		"env:push": "yarn env:push:development && yarn env:push:production",
		"env:push:development": "dotenv-vault push development .env.development",
		"env:push:production": "dotenv-vault push production .env.production",
		"env:pull": "yarn env:pull:example && yarn env:pull:development && yarn env:pull:production",
		"env:pull:example": "dotenv-vault pull example .env.example",
		"env:pull:development": "dotenv-vault pull development .env.development",
		"env:pull:production": "dotenv-vault pull production .env.production",
		"env:open": "dotenv-vault open",
		"env:login": "dotenv-vault login"
	},
	"dependencies": {
		"@eva-design/eva": "^2.1.1",
		"@react-navigation/native": "^6.0.13",
		"@react-navigation/native-stack": "^6.9.0",
		"@sentry/react-native": "4.2.2",
		"@ui-kitten/components": "^5.1.2",
		"@ui-kitten/eva-icons": "^5.1.2",
		"dotenv": "^16.0.3",
		"dotenv-expand": "^9.0.0",
		"eslint-plugin-jest": "^27.1.1",
		"expo": "^46.0.15",
		"expo-application": "~4.2.2",
		"expo-constants": "~13.2.4",
		"expo-dev-client": "~1.3.0",
		"expo-device": "~4.3.0",
		"expo-linear-gradient": "~11.4.0",
		"expo-location": "~14.3.0",
		"expo-network": "~4.3.0",
		"expo-secure-store": "~11.3.0",
		"expo-status-bar": "~1.4.0",
		"expo-system-ui": "~1.3.0",
		"expo-updates": "~0.14.6",
		"jest": "^26.6.3",
		"jest-expo": "^46.0.1",
		"react": "18.0.0",
		"react-dom": "18.0.0",
		"react-native": "0.69.6",
		"react-native-reanimated": "~2.9.1",
		"react-native-safe-area-context": "4.3.1",
		"react-native-screens": "~3.15.0",
		"react-native-svg": "12.3.0",
		"react-native-web": "~0.18.9",
		"sentry-expo": "~5.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.19.3",
		"@expo/config": "^7.0.0",
		"@svgr/cli": "^6.4.0",
		"@svgr/core": "^6.4.0",
		"@types/babel__core": "^7.1.19",
		"@types/eslint": "^8.4.6",
		"@types/jest": "^29.1.2",
		"@types/react": "~18.0.21",
		"@types/react-native": "~0.70.4",
		"@typescript-eslint/eslint-plugin": "^5.39.0",
		"@typescript-eslint/parser": "^5.39.0",
		"babel-plugin-module-resolver": "^4.1.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.25.0",
		"eslint-config-universe": "^11.1.0",
		"eslint-plugin-react": "^7.31.8",
		"renamer": "^4.0.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.8.4"
	},
	"resolutions": {
		"@types/react": "18.0.17",
		"@types/react-native": "0.69.5"
	},
	"private": true
}
